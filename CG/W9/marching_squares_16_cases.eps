% Función para dibujar un punto
/drawpoint {
  % x y radius color -> draws point
  setgray newpath 3 1 roll 2 copy 3 -1 roll 0 360 arc fill
} def

% Función para dibujar línea
/drawline {
  % x1 y1 x2 y2 width -> draws line
  setlinewidth newpath moveto lineto stroke
} def

% Función para dibujar cuadrado base
/drawsquare {
  % x y size -> draws square outline
  0.3 setgray 1 setlinewidth newpath
  3 copy moveto
  2 copy 0 exch rlineto
  2 copy exch 0 exch rlineto
  0 exch neg rlineto
  closepath stroke
} def

% Función para texto
/drawtext {
  % x y string -> draws text
  0 setgray /Helvetica 10 selectfont moveto show
} def

